{
    "name": "pow",
    "version": "1.0.0",
    "description": "A JS NES emulator developed from scratch. It features zero external dependencies and utilizes the latest JS features to ensure a thoroughly modern, powerful, and stable experience.",
    "type": "module",
    "main": "src/index.mjs",
    "exports": {
        ".": "./src/index.mjs"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:Leask/Pow.git"
    },
    "bugs": {
        "url": "https://github.com/Leask/Pow/issues"
    },
    "homepage": "https://github.com/Leask/Pow#readme",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "start": "node src/cli/run-headless.mjs --rom ./Mario.nes --frames 120",
        "smoke": "node src/cli/run-headless.mjs --rom ./Mario.nes --frames 120",
        "smoke:snes": "node src/cli/run-headless.mjs --rom \"./Mario World.smc\" --frames 240 --strict-opcodes",
        "test": "node --test",
        "test:snes:long": "node --test test/snes-kernel.long-run.test.mjs",
        "gui": "PORT=8184 node src/cli/serve-gui.mjs"
    },
    "keywords": [
        "nes",
        "emulator",
        "javascript",
        "headless"
    ],
    "author": "",
    "license": "MIT"
}
